{
    "workbench.iconTheme": "vscode-great-icons",
    "editor.tabSize": 2,
    "editor.wordSeparators":"`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
    "editor.renderWhitespace": "all",
    "editor.tabCompletion": "on",
    // + sync
    "sync.gist": "b6283dd70c926273fb25c06f062c130a",
    // - sync
    // - system
    // + path Intellisense
    "path-intellisense.autoSlashAfterDirectory": true,
    // - path Intellisense
    // + eslint
    "javascript.validate.enable": false,
    "files.associations": {
        "*.vue": "vue",
        "*.wxss": "css"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        "vue",
        "typescript",
        "typescriptreact"
    ],
    // - eslint
    // + vetur
    // - vetur
    // + vim
    "vim.leader": ";",
    "vim.useSystemClipboard": false,
    "vim.hlsearch": true,
    "vim.handleKeys": {
        "cmd+d": false,
        "cmd+f": false,
        "cmd+a": false,
        "<C-d>": false,
        "<C-a>": false,
        "<C-f>": false,
        "<C-n>": false
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        // + base
        {
            "before": [
                "<leader>",
                "e"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "after": [
                "%"
            ]
        },
        {
            "before": [
                "t",
                "n"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "t",
                "p"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "t",
                "q"
            ],
            "commands": [
                "workbench.action.closeOtherEditors"
            ]
        },
        {
            "before": [
                "<leader>",
                "2"
            ],
            "commands": [
                "workbench.view.explorer"
            ]
        },
        {
            "before": [
                "g",
                "c",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "<C-t>"
            ],
            "commands": [
                "workbench.action.files.newUntitledFile"
            ]
        },
        {
            "before": [
                "cmd+n"
            ],
            "commands": [
                "workbench.action.files.newUntitledFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "o"
            ],
            "commands": [
                "revealFileInOS"
            ]
        },
        {
            "before": [
                "<leader>",
                "q"
            ],
            "after": [
                ":",
                "q",
                "<enter>"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "m"
            ],
            "commands": [
                "workbench.action.editor.changeEOL"
            ]
        },
        // - base
        // + windows
        {
            "before": [
                "<leader>",
                "w",
                "v"
            ],
            "commands": [
                "workbench.action.splitEditor"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "s"
            ],
            "commands": [
                "workbench.action.splitEditorOrthogonal"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "e"
            ],
            "commands": [
                "workbench.actions.view.problems"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "a"
            ],
            "commands": [
                "workbench.action.terminal.toggleTerminal"
            ]
        },
        {
            "before": [
                "<leader>",
                "w",
                "p"
            ],
            "commands": [
                "workbench.view.extensions"
            ]
        },
        // - windows
        // + indent
        {
            "before": [
                "shift+tab"
            ],
            "commands": ["editor.action.outdentLines"]
        },
        {
            "before": [
                "tab"
            ],
            "commands": ["editor.action.indentLines"]
        },
        // - indent
        // + bookmarks
        {
            "before": [
                "m",
                "m"
            ],
            "commands": [
                "bookmarks.toggle"
            ]
        },
        {
            "before": [
                "m",
                "n"
            ],
            "commands": [
                "bookmarks.jumpToNext"
            ]
        },
        {
            "before": [
                "m",
                "N"
            ],
            "commands": [
                "bookmarks.jumpToPrevious"
            ]
        },
        {
            "before": [
                "m",
                "l"
            ],
            "commands": [
                "bookmarks.list"
            ]
        },
        {
            "before": [
                "m",
                "c"
            ],
            "commands": [
                "bookmarks.clear"
            ]
        },
        // - bookmarks
        // + project manager
        {
            "before": [
                "<leader>",
                "1"
            ],
            "commands": [
                "projectManager.listProjects"
            ]
        }
        // - project manager
    ],
    "vim.visualModeKeyBindings": [
        // + base
        {
            "before": [
                "<leader>",
                "e"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "/"
            ],
            "after": [
                "cmd+c",
                "/",
                "cmd+v",
                "<enter>"
            ]
        },
        {
            "before": [
                "<leader>",
                "e"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "after": [
                "%"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "after": [
                "%"
            ]
        },
        {
            "before": [
                "shift+tab"
            ],
            "commands": ["editor.action.outdentLines"]
        },
        {
            "before": [
                "tab"
            ],
            "commands": ["editor.action.indentLines"]
        },
        {
            "before": [
                "cmd+v"
            ],
            "after": [
                "d",
                "i",
                "cmd+v",
                "<Esc>"
            ]
        },
        // - base
        // + system handle
        {
            "before": [
                "cmd+x"
            ],
            "commands": [
                "editor.action.webvieweditor.cut",
                "extension.vim_escape",
                "extension.vim_escape"
            ]
        }
        // - system handle
    ],
    "editor.wordWrap": "off",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "eslint.migration.2_x": "off",
    "terminal.integrated.rendererType": "dom",
    "gitlab.instanceUrl": "https://git.yy.com",
    "gitlens.views.repositories.files.layout": "tree",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "gitlens.views.search.enabled": false,
    "gitlens.views.fileHistory.enabled": false,
    "gitlens.views.compare.enabled": false,
    "search.followSymlinks": false,
    "git.autorefresh": true,
    "window.zoomLevel": 1,
    "gitlens.advanced.messages": {
        "suppressGitMissingWarning": true
    },
    "editor.cursorStyle": "line",
    "editor.lineNumbers": "on",
    "workbench.editor.untitled.hint": "hidden"
    // - vim
}
