{
    // + system
    "editor.tabSize": 2,
    "window.zoomLevel": 1,
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.iconTheme": "vscode-great-icons",
    "editor.wordSeparators":"`~!@#$%^&*()=+[{]}\\|;:'\",.<>/?",
    "editor.renderWhitespace": "all",
    "editor.tabCompletion": "on",
    // - system
    // + path Intellisense
    "path-intellisense.autoSlashAfterDirectory": true,
    // - path Intellisense
    // + eslint
    "eslint.nodePath": "~/AppData/Roaming/npm/eslint",
    "eslint.run": "onSave",
    "editor.codeActionsOnSave": {
        "source.fixAll.tslint": true
    },
    "files.associations": {
        "*.vue": "vue",
        "*.wxml": "xml",
        "*.wxss": "css"
    },
    "eslint.validate": [
        "javascript",
        "javascriptreact",
        {
            "language": "vue",
            "autoFix": true
        }
    ],
    // - eslint
    // + vim
    "vim.leader": ";",
    "vim.useSystemClipboard": false,
    "vim.hlsearch": true,
    "vim.handleKeys": {
        "<C-d>": false
    },
    "vim.normalModeKeyBindingsNonRecursive": [
        // + base
        {
            "before": [
                "<leader>",
                "e"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "after": [
                "%"
            ]
        },
        {
            "before": [
                "t",
                "n"
            ],
            "commands": [
                "workbench.action.nextEditor"
            ]
        },
        {
            "before": [
                "t",
                "p"
            ],
            "commands": [
                "workbench.action.previousEditor"
            ]
        },
        {
            "before": [
                "t",
                "q"
            ],
            "commands": [
                "workbench.action.closeOtherEditors"
            ]
        },
        {
            "before": [
                "<leader>",
                "2"
            ],
            "commands": [
                "workbench.view.explorer"
            ]
        },
        {
            "before": [
                "g",
                "c",
                "c"
            ],
            "commands": [
                "editor.action.commentLine"
            ]
        },
        {
            "before": [
                "<C-t>"
            ],
            "commands": [
                "workbench.action.files.newUntitledFile"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                "workbench.action.files.newUntitledFile"
            ]
        },
        {
            "before": [
                "<leader>",
                "o"
            ],
            "commands": [
                "revealFileInOS"
            ]
        },
        {
            "before": [
                "<leader>",
                "q"
            ],
            "after": [
                ":",
                "q",
                "<enter>"
            ]
        },
        {
            "before": [
                "<C-b>"
            ],
            "commands": [
                "workbench.action.toggleSidebarVisibility"
            ]
        },
        {
            "before": [
                "<leader>",
                "c",
                "m"
            ],
            "commands": [
                "workbench.action.editor.changeEOL"
            ]
        },
        {
            "before": [
                "<C-a>"
            ],
            "commands": [
                "editor.action.selectAll"
            ]
        },
        {
            "before": [
                "<C-c>"
            ],
            "commands": [
                "editor.action.clipboardCopyAction"
            ]
        },
        {
            "before": [
                "<C-v>"
            ],
            "commands": [
                "editor.action.clipboardPasteAction"
            ]
        },
        {
            "before": [
                "<C-f>"
            ],
            "commands": [
                "actions.find"
            ]
        },
        // - base
        // + windows
        // - windows
        // + bookmarks
        {
            "before": [
                "m",
                "m"
            ],
            "commands": [
                "bookmarks.toggle"
            ]
        },
        {
            "before": [
                "m",
                "n"
            ],
            "commands": [
                "bookmarks.jumpToNext"
            ]
        },
        {
            "before": [
                "m",
                "N"
            ],
            "commands": [
                "bookmarks.jumpToPrevious"
            ]
        },
        {
            "before": [
                "m",
                "l"
            ],
            "commands": [
                "bookmarks.list"
            ]
        },
        {
            "before": [
                "m",
                "c"
            ],
            "commands": [
                "bookmarks.clear"
            ]
        },
        // - bookmarks
        // + project manager
        {
            "before": [
                "<leader>",
                "1"
            ],
            "commands": [
                "projectManager.listProjects"
            ]
        }
        // - project manager
    ],
    "vim.visualModeKeyBindings": [
        // + base
        {
            "before": [
                "<leader>",
                "e"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "/"
            ],
            "after": [
                "<C-c>",
                "/",
                "<C-v>",
                "<enter>"
            ]
        },
        {
            "before": [
                "v",
                "<leader>",
                "e"
            ],
            "after": [
                "$"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "v",
                "<leader>",
                "b"
            ],
            "after": [
                "^"
            ]
        },
        {
            "before": [
                "<leader>",
                "<leader>"
            ],
            "after": [
                "%"
            ]
        },
        {
            "before": [
                "v",
                "<leader>",
                "<leader>"
            ],
            "after": [
                "%"
            ]
        },
        {
            "before": [
                "<tab>"
            ],
            "commands": [
                "editor.action.indentLines"
            ]
        },
        {
            "before": [
                "shift+tab"
            ],
            "commands": [
                "editor.action.outdentLines"
            ]
        },
        {
            "before": [
                "<C-v>"
            ],
            "after": [
                "d",
                "i",
                "<C-v>",
                "<Esc>"
            ]
        }
        // - base
    ],
    // - vim
    "search.location": "panel",
    "editor.wordWrap": "on",
    "git.autofetch": true,
    "sync.gist": "f9c82beeab49c92d0cad56b209ce04a5",
    "[javascript]": {
        "editor.defaultFormatter": "HookyQR.beautify"
    },
}